<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yiqing.epidemicsystem.mapper.PersonMapper">

    <resultMap id="BaseResultMap" type="com.yiqing.epidemiccommon.pojo.Person">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="gender" column="gender" jdbcType="BIT"/>
            <result property="phone" column="phone" jdbcType="VARCHAR"/>
            <result property="address" column="address" jdbcType="VARCHAR"/>
            <result property="isStudent" column="is_student" jdbcType="BIT"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,gender,
        phone,address,is_student
    </sql>
</mapper>
